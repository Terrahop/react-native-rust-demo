[target.armv7-linux-androideabi]
ar = "$PWD/NDK/arm/bin/arm-linux-androideabi-ar"
linker = "$PWD/NDK/arm/bin/arm-linux-androideabi-gcc"

[target.i686-linux-android]
ar = "$PWD/NDK/x86/bin/i686-linux-android-ar"
